{{#each tandas}}
  <section class="ui stacked segment">
    <h2 class="ui header">
      {{title}}
    </h2>
    <p> Total Duration: {{duration}} </p>
    <a href="/tanda/{{id}}" class="ui icon button">
      <i class="icon folder open"></i>
    </a>
    <a href="/dashboard/deletetanda/{{id}}" class="ui icon button deltanda">
      <i class="icon trash"></i>
    </a>
  </section>
{{/each}}